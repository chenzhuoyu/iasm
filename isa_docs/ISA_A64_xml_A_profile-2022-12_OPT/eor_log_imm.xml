          <c colspan="5"></c>
        </box>
      </regdiagram>
      <encoding name="EOR_32_log_imm" oneofinclass="2" oneof="2" label="32-bit" bitdiffs="sf == 0 &amp;&amp; N == 0">
        <docvars>
          <docvar key="cond-setting" value="no-s" />
