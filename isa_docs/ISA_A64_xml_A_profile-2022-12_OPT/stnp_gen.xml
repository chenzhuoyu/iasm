    <explanation enclist="STNP_32_ldstnapair_offs" symboldefcount="1">
      <symbol link="sa_imm">&lt;imm&gt;</symbol>
      <account encodedin="imm7">
        <docvars>
          <docvar key="address-form-reg-type" value="signed-scaled-offset-pair-32" />
