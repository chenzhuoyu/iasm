if TSTATE.depth == 0 then
    UNDEFINED;

if TSTATE.depth == 1 then
    <a link="impl-aarch64.CommitTransactionalWrites.0" file="shared_pseudocode.xml" hover="function: CommitTransactionalWrites()">CommitTransactionalWrites</a>();
