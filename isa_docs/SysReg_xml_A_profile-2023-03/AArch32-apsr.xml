    <register execution_state="AArch32" is_register="True" is_internal="True" is_banked="False" is_stub_entry="False">
      <reg_short_name>APSR</reg_short_name>
        
        <reg_long_name>Application Program Status Register</reg_long_name>



      
        <reg_condition otherwise="UNDEFINED">when AArch32 is supported</reg_condition>
      



          <reg_reset_value></reg_reset_value>

      <reg_mappings>
        




      </reg_mappings>

        <reg_purpose>
          
    
      <purpose_text>
        <para>Hold program status and control information.</para>

      </purpose_text>
      <purpose_text>
        <note><para>Some of the fields in this register are permitted to return the value of the PSTATE field on a read. This is an exception to the general rule that an <arm-defined-word>UNKNOWN</arm-defined-word> field must not return information that cannot be obtained, at the current Privilege level, by an architected mechanism.</para></note>
      </purpose_text>
      <purpose_text>
        <para>For more information see <xref browsertext="'The Application Program Status Register, APSR'" filename="E_the_aarch32_application_level_programmers__.fm" linkend="CJAGBHBH"/>.</para>
      </purpose_text>

        </reg_purpose>

      <reg_groups>
            <reg_group>Process state registers</reg_group>
      </reg_groups>
      
      
        
      <reg_attributes>
          
    
      <attributes_text>
        <para>APSR is a 32-bit register.</para>
      </attributes_text>

      </reg_attributes>
