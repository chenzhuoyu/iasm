        <access_mechanism accessor="MRS PMCNTENSET_EL0" type="SystemAccessor">
            <encoding>
            <access_instruction>MRS &lt;Xt&gt;, PMCNTENSET_EL0</access_instruction>
                
                <enc n="op0" v="0b11"/>
                
