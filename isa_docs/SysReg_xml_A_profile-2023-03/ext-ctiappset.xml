        <reg_purpose>
          
    
      <purpose_text>
        <para>Sets the application triggers.</para>
      </purpose_text>

        </reg_purpose>

      <reg_groups>
          <reg_group>Cross-Trigger Interface registers</reg_group>
      </reg_groups>
      
      
        
