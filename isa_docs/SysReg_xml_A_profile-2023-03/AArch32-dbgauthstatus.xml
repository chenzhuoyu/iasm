            UNDEFINED;
        else
            AArch64.AArch32SystemAccessTrap(EL3, 0x05);
    else
        R[t] = DBGAUTHSTATUS;
elsif PSTATE.EL == EL3 then
    R[t] = DBGAUTHSTATUS;
                </pstext>
