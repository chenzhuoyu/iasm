        <docvar key="mnemonic" value="MSRR" />
      </docvars>
      <iclassintro count="1"></iclassintro>
      <arch_variants>
        <arch_variant name="FEAT_SYSREG128" feature="FEAT_SYSREG128" />
      </arch_variants>
