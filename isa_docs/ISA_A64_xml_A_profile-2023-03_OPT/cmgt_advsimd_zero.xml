          <docvar key="isa" value="A64" />
          <docvar key="mnemonic" value="CMGT" />
        </docvars>
        <asmtemplate><text>CMGT  </text><a link="sa_v" hover="Width specifier (field &quot;size&quot;) [D]">&lt;V&gt;</a><a link="sa_d" hover="SIMD&amp;FP destination register number (field &quot;Rd&quot;)">&lt;d&gt;</a><text>, </text><a link="sa_v" hover="Width specifier (field &quot;size&quot;) [D]">&lt;V&gt;</a><a link="sa_n" hover="SIMD&amp;FP source register number (field &quot;Rn&quot;)">&lt;n&gt;</a><text>, #0</text></asmtemplate>
