    <aliasref aliaspageid="CMPP_SUBPS" aliasfile="cmpp_subps.xml" hover="Compare with Tag" punct=".">
      <text>CMPP</text>
      <aliaspref>S == '1' &amp;&amp; Xd == '11111'</aliaspref>
    </aliasref>
    <alias_list_outro>
      <text>  See </text>
      <aliastablelink />
      <text> below for details of when the alias is preferred.</text>
    </alias_list_outro>
  </alias_list>
